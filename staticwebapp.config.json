{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*",
            "/*.css",
            "/*.js",
            "/*.png",
            "/*.jpg",
            "/*.svg",
            "/*.ico",
            "/*.json",
            "/api/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "text/javascript",
        ".css": "text/css"
    },
    "globalHeaders": {
        "cache-control": "public, max-age=31536000, immutable"
    }
}